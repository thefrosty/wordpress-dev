{
    "name": "johnpbloch/wordpress-dev",
    "description": "A library containing my standard development resources",
    "license": "MIT",
    "authors": [
        {
            "name": "John P Bloch",
            "email": "me@johnpbloch.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "johnpbloch/wordpress-core": ">=4.7.3",
        "antecedent/patchwork": "^1.3.4",
        "10up/wp_mock": "1.0.x-dev",
        "phpunit/phpunit": "*@stable"
    },
    "autoload": {
        "psr-4": {"JPB\\WP\\Dev\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "JPB\\Tests\\WP\\Dev\\": "tests/PHPUnit/",
            "JPB\\Mocks\\WP\\Dev\\": "tests/mocks"
        }
    }
}
